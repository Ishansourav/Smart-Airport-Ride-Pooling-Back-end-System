{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "smart-airport-ride-pooling",
  "compatibility_date": "2026-02-17",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "ride-pooling-db",
      "database_id": "local-dev-db",
      "migrations_dir": "migrations"
    }
  ]
}